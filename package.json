{
  "name": "where-is",
  "displayName": "Where is?",
  "description": "",
  "version": "0.0.4",
  "publisher": "rohanshresthaportpro",
  "repository": {
    "type": "git",
    "url": "https://github.com/rohanshrestha09/where-is.git"
  },
  "engines": {
    "vscode": ">=1.96.2",
    "cursor": ">=0.47.8"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Where is?",
      "properties": {
        "whereIs.enableHover": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable hover information for function calls"
        },
        "whereIs.enableDefinition": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable go to definition functionality"
        },
        "whereIs.enableDiagnostic": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable diagnostic warnings for naming conventions"
        },
        "whereIs.enabledProjects": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "base-project"
          ],
          "description": "List of project names (from package.json) where the extension should be enabled"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.21.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "acorn": "^8.14.1",
    "acorn-walk": "^8.3.4",
    "fuzzball": "^2.1.6",
    "glob": "^11.0.1"
  }
}
